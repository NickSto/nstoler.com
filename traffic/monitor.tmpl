 </head>

<body>

  <div id="container">
    <header>
        <TMPL_VAR NAVBAR>
    </header>
	
    <div id="main" role="main">
		<header>
			<h1 id="title">Traffic Monitor</h1>
			<p class="youarehere">
			<TMPL_IF END>
				Visits <TMPL_VAR START> to <TMPL_VAR END>
			</TMPL_IF>
			</p>
			<nav class="nav-options">
				<ul>
					<li>
						<a href="/misc/traffic.cgi?me=include">include me</a>
					</li>
			<TMPL_IF PREV>
					<li>
						<a href="/misc/traffic.cgi?p=<TMPL_VAR PREV>">previous page</a>
					</li>
			</TMPL_IF>
			<TMPL_IF NEXT>
					<li>
						<a href="/misc/traffic.cgi?p=<TMPL_VAR NEXT>">next page</a>
					</li>
			</TMPL_IF>
					</li>
				</ul>
			</nav>
		</header>
		
		<section id="notepad">
			<div id="visits">
			<TMPL_IF VISITS>
			<TMPL_LOOP VISITS>
				<article class="visit">
					<section class="pane1">
						<p>
							<span class="datum ip">
								<a href="http://whatismyipaddress.com/ip/<TMPL_VAR IP>#Geolocation-Map" target="_blank"><TMPL_VAR IP></a>
							</span>
						</p>
						<p>
							<span class="datum time">
								<TMPL_VAR TIME>
							</span>
						</p>
					</section>
					<section class="pane2">
						<p>
							<span class="datum">
								<span class="label">page:&nbsp;</span><TMPL_VAR PAGE>
							</span>
							<TMPL_IF COOKIE>
							<span class="datum">
								<span class="label">cookie:&nbsp;</span><TMPL_VAR COOKIE>
							</span>
							</TMPL_IF>
							<TMPL_IF LABEL>
							<span class="datum">
								<span class="label">label:&nbsp;</span><TMPL_VAR LABEL>
							</span>
							</TMPL_IF>
							<TMPL_IF REFERRER>
							<span class="datum">
								<span class="label">referrer:&nbsp;</span><TMPL_VAR REFERRER>
							</span>
							</TMPL_IF>
						</p>
						<p>
							<span class="datum">
								<span class="label">user-agent: </span><TMPL_VAR USER_AGENT>
							</span>
						</p>
					</section>
				</article>
			</TMPL_LOOP>
			<TMPL_ELSE>
				<article class="visit">
					<section class="pane1">
						<span class="datum">
							No visits to display!
						</span>
					</section>
				</article>
			</TMPL_IF>
			</div>
		</section>
		<nav class="nav-options">
			<ul>
				<li>
					<a href="/misc/traffic.cgi?me=include">include me</a>
				</li>
		<TMPL_IF PREV>
				<li>
					<a href="/misc/traffic.cgi?p=<TMPL_VAR PREV>">previous page</a>
				</li>
		</TMPL_IF>
		<TMPL_IF NEXT>
				<li>
					<a href="/misc/traffic.cgi?p=<TMPL_VAR NEXT>">next page</a>
				</li>
		</TMPL_IF>
				</li>
			</ul>
		</nav>
		<div class="after">
			<p><a href="clean.cgi">Clean</a> database</p>
			<p><a href="export-visits.cgi">Export</a> all visits</p>
		</div>
		</div> <!--! end of #main -->
	
		<footer>
			<TMPL_VAR NAVBAR>
		</footer>
	</div> <!--! end of #container -->


  <!-- JavaScript at the bottom for fast page loading -->

